config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="CernansPromise"
run/main_scene="res://assets/UI/Menu/MainMenu.tscn"
config/icon="res://icon.png"
boot_splash/main_loop_type=""

[autoload]

namelist="*res://core/autoload/namelist.gd"
logg="*res://core/autoload/log.gd"
options="*res://core/autoload/autoload-options.gd"
gamestate="*res://core/autoload/gamestate.gd"
utils="*res://core/autoload/utils.gd"
debug="*res://core/autoload/debug.gd"
UIManager="*res://core/autoload/UIManager.gd"
Input_Map="*res://core/autoload/input_maping.gd"

[compression]

formats/zlib/compression_level=5
formats/gzip/compression_level=5

[display]

window/size/height=700
window/stretch/mode="2d"
window/stretch/aspect="keep_height"

[editor]

convert_text_resources_to_binary_on_export=true

[editor_plugins]

enabled=PoolStringArray( "CeransDev", "HuePicker" )

